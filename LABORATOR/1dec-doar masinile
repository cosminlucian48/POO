#include <iostream>

using namespace std;

class Masini
{
private:
    string marca;
    string model;
public:
    Masini()
        {
            this->marca = "audi";
            this->model = "misto";
        }
    void afisare(){cout<<"Marca: "<<marca<<" .model: "<<model;}


};

class MasiniUndercover:public virtual Masini{
private:
    int viteza;
    int rezistenta;
public:
    MasiniUndercover():Masini(){
    this->viteza=10;
    this->rezistenta=20;
    }

    void afisare()
    {
        Masini::afisare();
        cout<<" .viteza: "<<viteza<<" .rezistenta: "<<rezistenta<<"\n";

    }


};

class MasiniInterventie:public virtual Masini{
private:
    int persoane;
    bool arma;
public:
    MasiniInterventie():Masini()
    {
        persoane=5;
        arma=true;
    }

    void afisare()
    {
        Masini::afisare();
        cout<<" .Persoane: "<<persoane<<" .arma: "<<arma<<"\n";

    }

};

class MasiniEmisieReceptie: public virtual Masini{
private:
    int acustic;
    int video;
public:
    MasiniEmisieReceptie():Masini()
    {
        acustic=100;
        video =400;
    }

    void afisare()
    {
        Masini::afisare();
        cout<<" .Acustic: "<<acustic<<" .video: "<<video<<"\n";

    }
};




